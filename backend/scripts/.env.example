# Environment Variables for AIOS Backend
# Copy this file to .env and customize as needed

# Database Configuration
POSTGRES_SERVER=localhost
POSTGRES_USER=aios
POSTGRES_PASSWORD=aios_password
POSTGRES_DB=aios_db
POSTGRES_PORT=5432

# TimescaleDB Configuration (if using separate instance)
TIMESCALE_SERVER=localhost
TIMESCALE_USER=aios
TIMESCALE_PASSWORD=aios_password
TIMESCALE_DB=aios_timeseries
TIMESCALE_PORT=5433

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Security Configuration
SECRET_KEY=your-super-secret-key-change-in-production-with-at-least-32-characters
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# CORS Configuration
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:5173","http://localhost:8080","http://localhost:4200"]

# Application Configuration
PROJECT_NAME="AIOS - Sistema de Análise e Inteligência Operacional"
VERSION=2.0.0
API_V1_STR=/api/v1

# WebSocket Configuration
WEBSOCKET_HEARTBEAT_INTERVAL=30

# Video Processing Configuration
MAX_CONCURRENT_STREAMS=10
RTSP_TIMEOUT=10

# Computer Vision Configuration
YOLO_MODEL_PATH=./models/yolov8n.pt
DETECTION_CONFIDENCE_THRESHOLD=0.5

# Celery Configuration
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=./logs/aios.log

# Development/Production
ENVIRONMENT=development
DEBUG=true
